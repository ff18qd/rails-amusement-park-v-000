<%= form_for @attraction do |f| %>
  <%= f.label :attraction_name %>:
  <%= f.text_field :name %><br />
  
  <%= f.label :attraction_min_height %>:
  <%= f.text_field :min_height %><br />

  <%= f.label :attraction_happiness_rating %>:
  <%= f.text_field :happiness_rating %><br />
  
  <%= f.label :attraction_nausea_rating %>:
  <%= f.text_field :nausea_rating %><br />
  
  <%= f.label :attraction_tickets %>:
  <%= f.text_field :tickets %><br />
  
  <%= f.submit %>
<% end %>